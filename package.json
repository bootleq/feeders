{
  "name": "feeders",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tsc": "tsc --noErrorTruncation",
    "pages:build": "pnpm next-on-pages",
    "preview": "pnpm pages:build && wrangler pages dev",
    "deploy": "pnpm pages:build && wrangler pages deploy",
    "db:tables": "pnpm wrangler d1 execute feeders --command 'PRAGMA table_list'",
    "db:delete": "rm -rf .wrangler/state/v3/d1/miniflare-D1DatabaseObject drizzle",
    "db:check": "LOCAL_DB_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit check",
    "db:migrate:gen": "LOCAL_DB_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit generate",
    "db:migrate:drop": "LOCAL_DB_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit drop",
    "db:migrate": "LOCAL_DB_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit migrate",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "cms": "cd ./directus && docker compose up --detach",
    "cms-log": "docker logs feeders-directus -f",
    "cms-stop": "cd ./directus && docker compose stop",
    "cms-restart": "cd ./directus && docker compose stop && docker compose up --detach && docker logs feeders-directus -f",
    "test": "vitest"
  },
  "dependencies": {
    "@auth/core": "^0.34.1",
    "@auth/d1-adapter": "^1.4.1",
    "@auth/drizzle-adapter": "^1.4.1",
    "@directus/sdk": "^17.0.1",
    "@floating-ui/react": "^0.26.20",
    "@heroicons/react": "^2.1.5",
    "@internationalized/date": "^3.5.5",
    "@next/bundle-analyzer": "^14.2.5",
    "@react-leaflet/core": "^2.1.0",
    "chanfana": "^2.0.2",
    "change-case": "^5.4.4",
    "css-select": "^5.1.0",
    "date-fns": "^3.6.0",
    "drizzle-orm": "^0.32.0",
    "fast-xml-parser": "^4.4.0",
    "framer-motion": "^11.3.19",
    "hono": "^4.4.12",
    "html-react-parser": "^5.1.16",
    "jotai": "^2.9.0",
    "jotai-scope": "^0.7.1",
    "jsondiffpatch": "^0.6.0",
    "leaflet": "^1.9.4",
    "leaflet-defaulticon-compatibility": "^0.1.2",
    "leaflet.markercluster": "^1.5.3",
    "linkify-react": "^4.1.3",
    "linkifyjs": "^4.1.3",
    "nanoid": "^5.0.7",
    "next": "14.1.0",
    "next-auth": "5.0.0-beta.19",
    "ngeohash": "^0.6.3",
    "ramda": "^0.30.1",
    "react": "^18",
    "react-aria-components": "^1.3.1",
    "react-dom": "^18",
    "react-leaflet": "^4.2.1",
    "react-leaflet-cluster": "^2.1.0",
    "use-debounce": "^10.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1",
    "@cloudflare/workers-types": "^4.20240620.0",
    "@faker-js/faker": "^8.4.1",
    "@react-aria/optimize-locales-plugin": "^1.1.1",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/react": "^16.0.0",
    "@types/better-sqlite3": "^7.6.11",
    "@types/leaflet": "^1.9.12",
    "@types/leaflet.markercluster": "^1.5.4",
    "@types/ngeohash": "^0.6.8",
    "@types/node": "^20",
    "@types/ramda": "^0.30.1",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.0.1",
    "better-sqlite3": "^11.1.2",
    "chalk": "^5.3.0",
    "drizzle-kit": "^0.23.0",
    "esbuild": "^0.23.0",
    "esbuild-register": "^3.5.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-next-on-pages": "^1.12.0",
    "jsdom": "^24.1.1",
