{
  "name": "feeders",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tsc": "tsc --noErrorTruncation",
    "worker:build": "NEXT_PUBLIC_CMS_BUILD_KEY=$(cat directus/build/BUILD_KEY) pnpm opennextjs-cloudflare build",
    "pull:charts": "tsx --env-file=.env.development scripts/pull_charts.mjs",
    "preview": "opennextjs-cloudflare preview",
    "upload:preview": "wrangler versions upload --preview-alias preview",
    "deploy": "tsx --env-file=.env.development scripts/deploy.mjs",
    "db:tables": "pnpm wrangler d1 execute feeders --command 'PRAGMA table_list'",
    "db:delete": "rm -rf .wrangler/state/v3/d1/miniflare-D1DatabaseObject drizzle",
    "db:check": "drizzle-kit check",
    "db:migrate:drop": "drizzle-kit drop",
    "db:migrate": "drizzle-kit migrate",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "cms": "cd ./directus && docker compose up --detach",
    "cms-log": "docker logs feeders-directus -f",
    "cms-stop": "cd ./directus && docker compose stop",
    "cms-restart": "cd ./directus && docker compose stop && docker compose up --detach && docker logs feeders-directus -f",
    "cms:build-local": "tsx --env-file=.env.development directus/scripts/build_to_r2.mjs",
    "cms:upload": "tsx --env-file=.env.development directus/scripts/sync_to_r2.mjs",
    "cms:snapshot": "cd ./directus && docker compose exec directus npx directus schema snapshot /directus/snapshots/$(date +'%Y%m%d').yml",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "test:shots": "playwright test --update-snapshots",
    "test": "pnpm run test:e2e"
  },
  "dependencies": {
    "@auth/core": "^0.34.2",
    "@auth/d1-adapter": "^1.4.2",
    "@bprogress/next": "^3.2.4",
    "@date-fns/tz": "^1.2.0",
    "@directus/sdk": "^17.0.1",
    "@floating-ui/react": "^0.26.22",
    "@heroicons/react": "^2.1.5",
    "@internationalized/date": "^3.5.5",
    "@next/bundle-analyzer": "^14.2.5",
    "@opennextjs/cloudflare": "^1.9.0",
    "@react-leaflet/core": "^2.1.0",
    "chanfana": "^2.1.0",
    "change-case": "^5.4.4",
    "css-select": "^5.1.0",
    "date-fns": "^3.6.0",
    "fast-xml-parser": "^4.4.1",
    "hono": "^4.5.4",
    "html-react-parser": "^5.1.16",
    "jotai": "^2.9.2",
    "jotai-scope": "^0.7.1",
    "jsondiffpatch": "^0.6.0",
    "leaflet": "^1.9.4",
    "leaflet-defaulticon-compatibility": "^0.1.2",
    "leaflet.markercluster": "^1.5.3",
    "linkify-react": "^4.3.2",
    "linkifyjs": "^4.3.2",
    "motion": "^12.23.24",
    "nanoid": "^5.0.7",
    "next": "14.2.16",
    "next-auth": "5.0.0-beta.25",
    "ngeohash": "^0.6.3",
    "ramda": "^0.30.1",
    "react": "^18.3.1",
    "react-aria-components": "^1.3.1",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1",
    "react-leaflet-cluster": "^2.1.0",
    "striptags": "4.0.0-alpha.4",
    "use-debounce": "^10.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@auth/drizzle-adapter": "^1.10.0",
    "@aws-sdk/client-s3": "^3.666.0",
    "@faker-js/faker": "^8.4.1",
    "@inquirer/confirm": "^5.1.9",
    "@playwright/test": "^1.50.1",
    "@react-aria/optimize-locales-plugin": "^1.1.1",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/react": "^16.0.0",
    "@types/better-sqlite3": "^7.6.11",
    "@types/geojson": "^7946.0.14",
    "@types/leaflet": "^1.9.12",
    "@types/leaflet.markercluster": "^1.5.4",
    "@types/ngeohash": "^0.6.8",
    "@types/node": "^20.14.14",
    "@types/ramda": "^0.30.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "better-sqlite3": "^11.1.2",
    "chalk": "^5.3.0",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.6",
    "esbuild": "^0.23.0",
    "esbuild-register": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.0",
    "jsdom": "^24.1.1",
    "postcss": "^8.4.41",
    "sass": "^1.77.8",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.9",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4",
    "vercel": "48.2.0",
    "vitest": "^2.0.5",
    "wrangler": "^4.40.2"
  },
  "packageManager": "pnpm@9.15.1+sha512.1acb565e6193efbebda772702950469150cf12bcc764262e7587e71d19dc98a423dff9536e57ea44c49bdf790ff694e83c27be5faa23d67e0c033b583be4bfcf"
}
